{
    "title": {
        "vehicle": {
            "label":    "Vehicle:",
            "format":   "{} ({}), {}: {}",
            "value":    [ "name", "id", "shortUserString", "description" ]
        },
        "chassis": {
            "label":    "Chassis:",
            "format":   "{} ({})",
            "value":    [ "name", "tag" ]
        },
        "turret": {
            "label":    "Turret:",
            "format":   "{} ({})",
            "value":    [ "name", "tag" ]
        },
        "gun": {
            "label":    "Gun:",
            "format":   "{} ({})",
